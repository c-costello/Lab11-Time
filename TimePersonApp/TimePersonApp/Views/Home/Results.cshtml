@model List<TimePersonApp.Models.Person>

<!DOCTYPE html>
<html>
<head>

</head>
<body>
    <header></header>
    <main>

        <table>
            <thead>
                <tr>
                    <td>Year</td>
                    <td>Honor</td>
                    <td>Name</td>
                    <td>Country</td>
                    <td>Birth Year</td>
                    <td>Death Year</td>
                    <td> Title</td>
                    <td>Categorty</td>
                    <td>Context</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var person in Model)
                {
                    <tr>
                        <td>@person.Year</td>
                        <td>@person.Honor</td>
                        <td>@person.Name</td>
                        <td>@person.Country</td>
                        <td>@person.BirthYear</td>
                        <td>@person.DeathYear</td>
                        <td>@person.Title</td>
                        <td>@person.Category</td>
                        <td>@person.Context</td>
                    </tr>
                }

            </tbody>
        </table>
    </main>
    <footer></footer>
</body>
</html>

